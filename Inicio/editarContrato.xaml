<Window x:Class="Inicio.editarContrato"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inicio"
        mc:Ignorable="d"
        Title="Modificar Datos Contrato" Height="615.813" Width="636.361">
    <Grid>
        <Button x:Name="btnEditarCont" Content="Editar Contrato" HorizontalAlignment="Left" Margin="435,92,0,0" VerticalAlignment="Top" Width="115" Height="35" IsEnabled="False" Click="btnEditarCont_Click" />
        <Label x:Name="label3" Content="Rut Cliente" HorizontalAlignment="Left" Margin="68,26,0,0" VerticalAlignment="Top" Width="87"/>
        <!--<Label x:Name="label4" Content="Fecha Inicio Vigencia" HorizontalAlignment="Left" Margin="67,264,0,0" VerticalAlignment="Top"/>-->
        <Label x:Name="label7" Content="Declaracion de Salud" HorizontalAlignment="Left" Margin="69,267,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label8" Content="Prima Anual" HorizontalAlignment="Left" Margin="69,327,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label9" Content="Prima mensual" HorizontalAlignment="Left" Margin="69,385,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.057,0.447"/>
        <ComboBox x:Name="cbbRutCli" HorizontalAlignment="Left" Margin="219,26,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbbRutCli_SelectionChanged"/>
        <Label x:Name="label10" Content="Plan" HorizontalAlignment="Left" Margin="69,159,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.054,0.45"/>
        <TextBox x:Name="txtPrimaAnu" HorizontalAlignment="Left" Height="23" Margin="220,328,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <TextBox x:Name="txtPrimaMen" HorizontalAlignment="Left" Height="23" Margin="220,384,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <ComboBox x:Name="cbbPlan" HorizontalAlignment="Left" Margin="218,160,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbbPlan_SelectionChanged" IsEnabled="False"/>
        <Button x:Name="btnListarCon" Content="Listar Contrato" HorizontalAlignment="Left" Margin="435,156,0,0" VerticalAlignment="Top" Width="115" Height="35" Click="btnListarCon_Click"/>
        <!--<DatePicker x:Name="dtpFechaInicio" HorizontalAlignment="Left" Margin="218,262,0,0" VerticalAlignment="Top" IsEnabled="False"/>-->
        <ComboBox x:Name="cbbSalud" HorizontalAlignment="Left" Margin="219,268,0,0" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <ComboBox x:Name="cbbListaContrato" HorizontalAlignment="Left" Margin="218,108,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbbListaContrato_SelectionChanged" />
        <Label Content="Numero Contrato" HorizontalAlignment="Left" Margin="68,115,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnBuscarCont" Content="Buscar Contrato" HorizontalAlignment="Left" Margin="435,34,0,0" VerticalAlignment="Top" Width="115" Height="33" Click="btnBuscarCont_Click"/>
        <Button x:Name="btnTerminarContrato" Content="Terminar Contrato" HorizontalAlignment="Left" Margin="435,244,0,0" VerticalAlignment="Top" Width="115" Height="31" IsEnabled="False" Click="btnTerminarContrato_Click"/>
        <Button x:Name="btnLimpiar" Content="Limpiar" HorizontalAlignment="Left" Margin="435,323,0,0" VerticalAlignment="Top" Width="115" Height="37" Click="btnLimpiar_Click"/>
        <Label Content="Nombre Cliente" HorizontalAlignment="Left" Margin="67,67,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNombreCliCon" HorizontalAlignment="Left" Height="23" Margin="219,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <Label Content="Poliza" HorizontalAlignment="Left" Margin="70,211,0,0" VerticalAlignment="Top" Width="86"/>
        <TextBox x:Name="txtPoliza" HorizontalAlignment="Left" Height="23" Margin="218,211,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <Label Content="Observaciones" HorizontalAlignment="Left" Margin="69,444,0,0" VerticalAlignment="Top" Width="88"/>
        <TextBox x:Name="txtObsv" HorizontalAlignment="Left" Height="89" Margin="218,444,0,-17.8" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" IsEnabled="False"/>

    </Grid>
</Window>
